    "@vueuse/core": "^9.5.0",
    "bootstrap": "^5.2.2",
    "bootstrap-vue": "^2.23.1",
    "bootstrap-vue-3": "^0.4.6",
    "uuidv4": "^6.2.13",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@testing-library/vue": "^6.6.1",
